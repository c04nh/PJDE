    <title>프젝돔이</title>
  <body>
    <nav class="header">
      <div class="fake" style="text-align: center">
        <img
          src="img/fake.png"
          style="height: 60px; width: 60px"
        />
      </div>
      <div class="logo" style="text-align: center">
        <a href="main">
          <img class="img"
          src="img/logo.png"
          style="height: 60px; width: 60px"
          />
        <span class="pjde">프젝돔이</span></a>
      </div>
      <div class="notice_icon">
        <a href="main/notice"
          ><img
            src="/img/notice.png"
            style="height: 40px; width: 40px"
        /></a>
      </div>
  </nav>
    <div id = banner>
      <% if(notice.length == 0){%>
        <div id = "emptyNotice">공지사항이 없습니다.</div>
      <%}else{%>
        <p class="title">
          <%= notice[0].content;%>
        </p>
        <p class="project">
            <%= notice[0].proName;%>
        </p>
        <div class="name_date">
          <span class="name">작성자: <%= notice[0].userId %></span>
          <span class="sep">|</span>
          <span class="date"><%= notice[0].notDate %></span>
        </div>
      <%}%>
    </div>
    <div id="top">
      <% if(result.length == 0){ %>
        <div id="empty">프로젝트가 없습니다.</div>
      <%}else{%>
        <% for (var i = 0; i < result.length; i++){ %>
          <div class="column">
            <div class="pj" style="background-color:<%= result[i].col;%>">
                <a class = "info" href="main/<%= result[i].proId;%>/calender">
                  <h2 class="title"><%= result[i].proName;%></h2>
                    <p style="font-size: 13pt">
                      팀원 : <%= result[i].manager;%> <%= result[i].userId1;%> <%= result[i].userId2;%> <%= result[i].userId3;%> <%= result[i].userId4;%><br><br>
                      진행상태 :  <%= result[i].progress;%>
                    </p>
                </a>
            </div>
          </div>
        <% } %>
      <% } %>
    </div>
    <div class="addButton" style="position: fixed; bottom: 20px; right: 20px">
      <a href="addProject"
        ><img src="/img/plus.png" title="프젝 추가" class="plus"
      /></a>
    </div> 
    <p>
